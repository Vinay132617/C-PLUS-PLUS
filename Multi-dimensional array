#include <iostream>

using namespace std;

int main()
{
    int marks[3][5]
    {    
        {6,7,8,9,7},
        {7,6,8,7,9},
        {5,4,6,7,8}
    };
    
    cout<<marks[0][0]<<"  "<<marks[0][1]<<"  "<<marks[0][2]<<"  "<<marks[0][3]<<"  "<<marks[0][4]<<endl;
    cout<<marks[1][0]<<"  "<<marks[1][1]<<"  "<<marks[1][2]<<"  "<<marks[1][3]<<"  "<<marks[1][4]<<endl;
    cout<<marks[2][0]<<"  "<<marks[2][1]<<"  "<<marks[2][2]<<"  "<<marks[2][3]<<"  "<<marks[2][4]<<endl;
    return 0;
}
